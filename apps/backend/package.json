{
  "name": "@skyvix/backend",
  "version": "0.0.1",
  "description": "Skyvix Backend API",
  "private": true,
  "packageManager": "bun@1.0.0",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:generate:dev": "dotenv -e .env.development -- prisma generate",
    "db:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "db:migrate": "dotenv -e .env -- prisma migrate deploy",
    "db:push:dev": "dotenv -e .env.development -- prisma db push",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:studio:dev": "dotenv -e .env.development -- prisma studio",
    "db:reset": "dotenv -e .env -- prisma migrate reset",
    "db:reset:dev": "dotenv -e .env.development -- prisma migrate reset",
    "db:pull": "dotenv -e .env -- prisma db pull",
    "db:pull:dev": "dotenv -e .env.development -- prisma db pull",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/websockets": "^11.1.3",
    "@prisma/client": "^6.9.0",
    "@skyvix/shared": "workspace:*",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "jest": "30.0.0",
    "ioredis": "^5.6.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.2",
    "xendit-node": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.4",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.7.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^29.5.0",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}